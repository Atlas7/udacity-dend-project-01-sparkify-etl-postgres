{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/msd_song.schema.json",
  "title": "Simulated Million Song Dataset User log",
  "description": "Simulated user log file generated by the Eventsim event simulator (https://github.com/Interana/eventsim) based on the songs in the Million Song Dataset. These simulate activity logs from a music streaming app based on specified configurations.",
  "type": "object",
  "properties": {
    "artist": {
      "description": "Song Artist",
      "type": "string"
    },
    "auth": {
      "description": "User Authentication Type",
      "type": "string"
    },
    "firstName": {
      "description": "User First Name",
      "type": "string"
    },
    "gender": {
      "description": "User Gender",
      "type": "string"
    },
    "itemInSession": {
      "description": "Item in Session",
      "type": "integer"
    },
    "lastName": {
      "description": "User First Name",
      "type": "string"
    },
    "length": {
      "description": "User duration in seconds",
      "type": "number"
    },
    "level": {
      "description": "Price plan of the user",
      "type": "string"
    },
    "location": {
      "description": "User location",
      "type": "string"
    },
    "method": {
      "description": "User HTTP method (e.g. GET, PUT, etc)",
      "type": "string"
    },
    "page": {
      "description": "Which page the user is on",
      "type": "string"
    },
    "registration": {
      "description": "User registration",
      "type": "number"
    },
    "sessionId": {
      "description": "User session ID",
      "type": "integer"
    },
    "song": {
      "description": "Song ID user listening to",
      "type": "string"
    },
    "status": {
      "description": "HTTP status",
      "type": "integer"
    },
    "ts": {
      "description": "User session start time in UNIX Epoch time",
      "type": "int"
    },
    "userAgent": {
      "description": "How the user access the app (e.g. browser and device versions)",
      "type": "string"
    },
    "userId": {
      "description": "user ID",
      "type": "integer"
    }
  },
  "required": ["userId", "session_id", "ts"]
}